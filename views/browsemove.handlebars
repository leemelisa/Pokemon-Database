<div class="container">
  <br>
	
  <p>
    <h3><center>(Pokemon) Moves in Your Database</center></h3>
    <table class="table">
      <thead>
        <tr>
          {{#each sqlCol}}
		<th scope="col">{{this.name}}</th>	
          {{/each}}
        </tr>
       </thead>

       <tbody>
	 {{#each sqlResult}}
	   <tr>
	    <td>{{this.ID}}</td>
	    <td><a href="?key=Name&value='{{this.Name}}'">{{this.Name}}</a></td>
	    <td><a href="?key=AttackPower&value={{this.AttackPower}}">{{this.AttackPower}}</a></td>
	    <td><a href="?key=PP&value={{this.PP}}">{{this.PP}}</a></td>
	    <td><a href="?key=Description&value='{{this.Description}}'">{{this.Description}}</a></td>
	   </tr>    	

  	 {{/each}}
        </tbody>
       </table>
	   
	{{#if filter}}
		<a href="/browsemove" class="btn btn-primary">Reset filter</a>
	{{/if}}
	
  </p>

</div>


